# MongoDB Atlas API Configuration
# Get your API keys from: https://cloud.mongodb.com/v2#/account/publicApi

# Atlas API Public Key
ATLAS_PUBLIC_KEY=

# Atlas API Private Key  
ATLAS_PRIVATE_KEY=

# Atlas Organization ID
ATLAS_ORG_ID=

# Atlas API Base URL (typically no need to change)
ATLAS_API_BASE_URL=https://cloud.mongodb.com/api/atlas/v2

# Optional: Logging configuration
LOG_LEVEL=INFO
LOG_DIR=logs